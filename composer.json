{
	"name": "flexsyscz/forms",
	"description": "Helpful classes for better using of Nette Forms.",
	"keywords": ["flexsyscz", "model", "database", "entity", "repository", "facade"],
	"homepage": "https://www.flexsys.cz",
	"license": ["BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"authors": [
		{
			"name": "Václav Lukeš",
			"homepage": "https://www.vaclavlukes.cz"
		}
	],
	"require": {
		"php": ">=8.1",
		"nette/application": "^3.2",
		"nette/forms": "^3.2",
		"nextras/dbal": "^5.0"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"phpstan/phpstan-nette": "^2.0",
		"nette/coding-standard": "^3.3"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"minimum-stability": "stable",
	"scripts": {
		"phpstan": "phpstan analyse",
		"tester": "tester tests -s"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0-dev"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
